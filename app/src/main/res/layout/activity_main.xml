<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuActivity"
    tools:ignore="ExtraText">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto"
        android:text="Bienvenido Leonardo!"
        android:textColor="@color/blueapp"
        android:textSize="25dp"
        android:textStyle="bold"
         app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.094"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.055" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="¿Qué desea hacer?"
        android:textColor="@color/blueapp"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints" />

    <Button
    android:id="@+id/btn_rInventario"
    android:layout_width="388dp"
    android:layout_height="79dp"
    android:layout_marginTop="60dp"
    android:background="@drawable/asdf"
    android:gravity="center"
    android:minHeight="48dp"
    android:text="Revisar Inventario"
    android:textAllCaps="false"
    android:textColor="@color/blueapp"
    android:textSize="20dp"
    android:textStyle="bold"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView"
    tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/btn_IngProducto"
        android:layout_width="388dp"
        android:layout_height="79dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/btexport"
        android:gravity="center"
        android:minHeight="48dp"
        android:text="Entrada de almacén"
        android:textAllCaps="false"
        android:textColor="@color/blueapp"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_rInventario"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/btn_SalProducto"
        android:layout_width="388dp"
        android:layout_height="79dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/btnsalida"
        android:gravity="center"
        android:minHeight="48dp"
        android:text="Salida de almacén"
        android:textAllCaps="false"
        android:textColor="@color/blueapp"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_IngProducto"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/btn_report"
        android:layout_width="388dp"
        android:layout_height="79dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/btnreg"
        android:gravity="center"
        android:minHeight="48dp"
        android:text="Revisión de Ambientes"
        android:textAllCaps="false"
        android:textColor="@color/blueapp"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_SalProducto"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/btn_reportPersonal"
        android:layout_width="388dp"
        android:layout_height="79dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/rvpersonal"
        android:gravity="center"
        android:minHeight="48dp"
        android:text="Revisión de Personal"
        android:textAllCaps="false"
        android:textColor="@color/blueapp"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_report"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>